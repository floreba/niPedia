<%= form_tag notes_path, method: :get, class: "d-flex" do %>
  <%= text_field_tag :query,
    params[:query],
    class: "form-control",
    placeholder: "Find a note"
  %>
  <%= submit_tag "search", class: "material-symbols-outlined" %>
<% end %>


